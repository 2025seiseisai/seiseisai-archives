@import "../scss/bootstrap.scss";

@function pc-get($size, $viewport: 992){
  @return 100 / $viewport
}

  

@mixin pc($font_size: 10) {
  font-size: $font_size * 1px;
  font-size: pc-get($font_size);
}

@function mob-get($size, $viewport: 576){
    @return 100 / $viewport
  }
  
    
  
  @mixin mob($font_size: 10) {
    font-size: $font_size * 1px;
    font-size: mob-get($font_size);
  }

